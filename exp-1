#include <stdio.h>
#include <string.h>

int main() {
    int n, i, j, len, palindrom;
    char words[100][100];
    printf("Enter number of words: ");
    scanf("%d", &n);
    printf("Enter %d words:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%s", words[i]);
    }
    for (i = 0; i < n; i++) {
        len = strlen(words[i]);
        palindrom = 1; 
        for (j = 0; j < len / 2; j++) {
            if (words[i][j] != words[i][len - j - 1]) {
                palindrom = 0;
                break;
            }
        }
        if (palindrom == 1) {
            printf("First palindromic string: %s\n", words[i]);
            return 0;
        }
    }
    printf("No palindromic string found.\n");
    return 0;
}
