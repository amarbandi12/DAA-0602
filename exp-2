#include <stdio.h>
int main() {
    int n, m, i, j;
    printf("Enter size of nums1: ");
    scanf("%d", &n);
    int nums1[n];
    printf("Enter elements of nums1: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &nums1[i]);
    }
    printf("Enter size of nums2: ");
    scanf("%d", &m);
    int nums2[m];
    printf("Enter elements of nums2: ");
    for (i = 0; i < m; i++) {
        scanf("%d", &nums2[i]);
    }
    int answer1 = 0, answer2 = 0;
    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            if (nums1[i] == nums2[j]) {
                answer1++;
                break;
            }
        }
    }
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            if (nums2[i] == nums1[j]) {
                answer2++;
                break; 
            }
        }
    }
    printf("Output: [%d,%d]\n", answer1, answer2);
    return 0;
}
